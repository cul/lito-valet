<h3>The following logs are available.</h3>

<table class='table table-striped table-hover table-condensed'>

  <thead>
    <tr>
      <th></th>      
      <th>total log entries</th>      
    </tr>
  </thead>

  <tbody>
    <% @logset_counts.each do |logset, count| %>
      <tr>
        <td>
          <h4>
            <%= link_to logset, logs_path(logset: logset) %>
          </h4>
        </td>
        <td>
          <%= count%>
        </td>
      </tr>
    <% end %>
  </tbody>

</table>


  