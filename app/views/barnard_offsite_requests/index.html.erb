<p id="notice"><%= notice %></p>

<h4>Listing Offsite Requests</h4>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offsite_requests.each do |offsite_request| %>
      <tr>
        <td><%= link_to 'Show', offsite_request %></td>
        <td><%= link_to 'Edit', edit_offsite_request_path(offsite_request) %></td>
        <td><%= link_to 'Destroy', offsite_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offsite request', new_offsite_request_path %>
